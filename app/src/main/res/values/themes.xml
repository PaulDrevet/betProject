<resources>
    <!-- Base application theme. -->
    <style name="Theme.BetProject" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/yellowH</item>
        <item name="colorPrimaryVariant">@color/yellowH</item>
        <item name="colorOnPrimary">@color/yellowH</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/orangeC</item>
        <item name="colorAccent">@color/silverR</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="BottomNavigationTheme" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">@color/red</item>
        <item name="colorAccent">@color/orangeC</item>
    </style>
</resources>